{
    "configurations": [
        {
            "name": "AI-Artworks-Windows-MSVC",
            "includePath": [
                "${workspaceFolder}/AI-ARTWORKS/include/**",
                "${workspaceFolder}/AI-ARTWORKS/src/**",
                "${env:VCPKG_ROOT}/installed/x64-windows/include",
                "${env:WindowsSDKDir}Include/${env:WindowsSDKVersion}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "_WIN32_WINNT=0x0A00",
                "WIN32_LEAN_AND_MEAN",
                "_CRT_SECURE_NO_WARNINGS",
                "NOMINMAX",
                "AI_ARTWORKS_VERSION_MAJOR=1",
                "AI_ARTWORKS_VERSION_MINOR=0",
                "AI_ARTWORKS_VERSION_PATCH=0"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-msvc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/AI-ARTWORKS/include",
                    "${workspaceFolder}/AI-ARTWORKS/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "AI-Artworks-Windows-GCC",
            "includePath": [
                "${workspaceFolder}/AI-ARTWORKS/include/**",
                "${workspaceFolder}/AI-ARTWORKS/src/**",
                "${env:VCPKG_ROOT}/installed/x64-windows/include"
            ],
            "defines": [
                "_DEBUG",
                "_WIN32_WINNT=0x0A00",
                "WIN32_LEAN_AND_MEAN",
                "_CRT_SECURE_NO_WARNINGS",
                "NOMINMAX"
            ],
            "compilerPath": "gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/AI-ARTWORKS/include",
                    "${workspaceFolder}/AI-ARTWORKS/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}