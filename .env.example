# Example .env file for AISIS configuration overrides
#
# Copy this to .env and modify values as needed.
# These will override settings in config.json

# Voice settings
VOICE_WHISPER_MODEL=small
VOICE_TTS_ENGINE=bark
VOICE_SAMPLE_RATE=16000
VOICE_LANGUAGE=en
VOICE_CHUNK_SIZE=30

# LLM settings
LLM_MODEL_NAME=mixtral-8x7b
LLM_QUANTIZED=true

# GPU settings
GPU_USE_CUDA=true
GPU_DEVICE_ID=0

# Paths
PATHS_MODELS_DIR=models
PATHS_CACHE_DIR=cache
PATHS_TEXTURES_DB=textures/textures.sqlite

# UI settings
UI_THEME=dark
UI_WINDOW_SIZE=1280,720
