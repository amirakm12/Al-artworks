# Example .env file for AISIS
# Copy to .env and customize

MODELS_DIR=models
CACHE_DIR=cache
LOGS_DIR=logs
OUTPUTS_DIR=outputs

LLM_MODEL_NAME=mixtral-8x7b
LLM_QUANTIZED=true

WHISPER_MODEL=small
TTS_ENGINE=bark
SAMPLE_RATE=16000
LANGUAGE=en
CHUNK_SIZE=30

USE_CUDA=true
GPU_DEVICE_ID=0

UI_THEME=dark
UI_WINDOW_WIDTH=1280
UI_WINDOW_HEIGHT=720

DATABASE_URL=sqlite:///aisis.db
API_RATE_LIMIT=100
CORS_ALLOWED_ORIGINS=*
